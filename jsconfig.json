{
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true,
		"target": "ES2020",
		"module": "ES2020",
		"moduleResolution": "node",
		"typeRoots": ["./node_modules/@types", "./node_modules/@discordjs/typings"]
	}
}
